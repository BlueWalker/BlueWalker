<?xml version="1.0" encoding="UTF-8"?>
<manifest>
    <remote name="bw" fetch="https://github.com/BlueWalker"/>

    <default revision="refs/heads/master"
        remote="bw"
        sync-j="4"/>

    <!-- Gradle -->
    <project path="." name="GradleBaseProject"
        remote="bw" revision="master" />
    <!-- Libraries -->
    <project path="Libraries/Core" name="Core"
        remote="bw" revision="master" />
    <project path="Libraries/UserTrackerModule" name="UserTrackerModule"
        remote="bw" revision="master" />
    <project path="Libraries/Trilateration" name="Trilateration"
        remote="bw" revision="master" />
    <project path="Libraries/Pathfinding" name="Pathfinding"
        remote="bw" revision="master" />
    <project path="Libraries/BeaconLib" name="BeaconLib"
        remote="bw" revision="master" />
    <!-- Examples -->
    <project path="Examples" name="Examples"
        remote="bw" revision="master" />
    <!-- Apps -->
    <project path="Apps/GlassApplication" name="GlassApplication"
        remote="bw" revision="master" />
</manifest>
